{
  "expo": {
    "name": "Vazonao",
    "slug": "Vazonao",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/vazonao-v3.png",
    "scheme": "vazonao",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/placeholder-album.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "softwareKeyboardLayoutMode": "pan",
      "supportsTablet": true,
      "bundleIdentifier": "com.andydevlabs.Vazonao"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/vazonao-v3.png",
        "backgroundColor": "#5D8AA8"
      },
      "package": "com.andydevlabs.Vazonao"
    },
    "web": {
      "favicon": "./assets/placeholder-album.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
